global
        ulimit-n  51200
        
defaults
        log  global
        mode  tcp
        option  dontlognull
        timeout connect 1000
        timeout client 150000
        timeout server 150000
        
frontend ss-in
        bind *:{{ internal_node_port }}
        default_backend ss-out
        
backend ss-out
        server server1 {{ external_node_domain }}:{{ external_node_port }} maxconn 20480
